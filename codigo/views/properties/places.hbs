<div class="fixed-action-btn">
  <a class="btn-floating blue btn-large" onclick="location.href='/create'">
    <i class="material-icons large">add</i>
  </a>
</div>

</section> 
<div class="row container">
  <div class="col s6 gray">
    <div class="card">
    {{#each places as |place|}}
      <div class="card-image">
        <img src="{{photo}}" alt="">
        <a class="btn-floating halfway-fab waves-effect waves-light red" href="/places/{{_id}}">
          <i class="material-icons">add</i>
        </a> 
      </div>
      <span class="card-title ">{{name}}</span>
      {{/each}}
    </div>
  </div>
</div>

